<!--- create edit cutting popup (cecpp) --->
<div class="popup" id="cecpp">    
    <div class="popupContent t-120 w-600 h-450">

        <div class="popupIcons">
            <div class="popupIcon3"><i class="fa-solid fa-square-plus"></i></div>
            <div class="popupClose1" id="cecppClose"><i class="fa-solid fa-rectangle-xmark"></i></div>
        </div>
        
        <div class="popupTitle1" id="cecppTitle"></div>

        <div class="divInputsColumn mtm mbm">
            <div class="divInput">
                <label class="label1" id="cecppDateLabel">Fecha</label>
                <input type="date" class="input1 ixxxl ipl" id="cecppDate" autocomplete="off">
                <div class="divError" id="cecppDateError"></div>
            </div>
            <div class="divInput mtm">
                <label class="label1" id="cecppCuttingLabel">Corte</label>
                <input type="text" class="input1 ixxxl ipl" id="cecppCutting" autocomplete="off">
                <div class="divError" id="cecppCuttingError"></div>
            </div>
            <div class="divInput mtm">
                <label class="label1" id="cecppMoldLabel">Molde</label>
                <input type="text" class="input1 ixxxl ipl" id="cecppMold" autocomplete="off">
                <div class="divError" id="cecppMoldError"></div>
                <ul class="predictedElements" id="cecppMoldPredicted"></ul>
            </div>
            <div class="divInput mtm">
                <label class="label1" id="cecppDescriptionLabel">Descripci√≥n</label>
                <input type="text" class="input1 ixxxl ipl" id="cecppDescription">
                <div class="divError" id="cecppDescriptionError"></div>
            </div>
        </div>

        <div class="divButtonsRow m-b-10">
            <div id="cecppCreate" class="button1 bxs">Crear</div>
            <div id="cecppEdit" class="button1 bxs">Editar</div>
        </div>
    </div>
</div>
